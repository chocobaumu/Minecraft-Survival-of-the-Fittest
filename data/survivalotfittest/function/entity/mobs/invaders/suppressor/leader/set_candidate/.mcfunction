# 決定
scoreboard players operation @n[type=marker,tag=Choco.SotF.Invasion.FrontLineCandidate,tag=Choco.SotF.Invasion.Decided] Choco.SotF.ScoreStorage.1 = @s Choco.SotF.ScoreStorage.1
scoreboard players operation @n[type=marker,tag=Choco.SotF.Invasion.FrontLineCandidate,tag=Choco.SotF.Invasion.Decided] Choco.SotF.ScoreStorage.2 = @s Choco.SotF.ScoreStorage.2
scoreboard players operation @n[type=marker,tag=Choco.SotF.Invasion.FrontLineCandidate,tag=Choco.SotF.Invasion.Decided] Choco.SotF.ScoreStorage.3 = @s Choco.SotF.ScoreStorage.3
scoreboard players operation @n[type=marker,tag=Choco.SotF.Invasion.FrontLineCandidate,tag=Choco.SotF.Invasion.Decided] Choco.SotF.ScoreStorage.4 = @s Choco.SotF.ScoreStorage.4
execute as @n[type=marker,tag=Choco.SotF.Invasion.FrontLineCandidate,tag=Choco.SotF.Invasion.Decided] run tag @s add Choco.SotF.Invasion.FrontLine
execute as @n[type=marker,tag=Choco.SotF.Invasion.FrontLineCandidate,tag=Choco.SotF.Invasion.Decided] run tag @s remove Choco.SotF.Invasion.FrontLineCandidate